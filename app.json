{
  "expo": {
    "name": "perioscan",
    "slug": "perioscan",
    "version": "1.0.0",
    "runtimeVersion": "51.0.0",
    "orientation": "portrait",
    "icon": "./assets/icone-perioscan-mobile.png",
    "userInterfaceStyle": "light",
    "scheme": "perioscan",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/icone-perioscan-mobile.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.suaempresa.perioscan"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icone-perioscan-mobile.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.suaempresa.perioscan"
    },
    "web": {
      "favicon": "./assets/icone-perioscan-mobile.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
}
